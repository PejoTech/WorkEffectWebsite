@using WorkEffect.Website.Types
@model WorkEffect.Website.Types.CmsPage

@{
    ViewBag.Title = "Details";
}

<div class="row">
    @{
        foreach (var part in Model.CmsContents.OrderBy(a => a.Index))
        {
            @Html.Partial("_contentDetails", part)
            ;
        }
    }

    @Html.Partial("_ContentCreate", new CmsContent(Model.Id, NavPages.Count + 1))  
</div>
<p>
    @Html.ActionLink("Edit", "Edit", new { id = Model.Id }) |
    @Html.ActionLink("Back to List", "Index")
</p>