@using WorkEffect.Website.Models

@foreach (var content in (List<Content>)ViewBag.Sections)
{
    <section id="@content.Section.Name" class="container content-section text-center">
        <div class="row">
            <div class="col-lg-8 col-lg-offset-2">
                <p>
                    @content.Text;
                </p>
            </div>
        </div>
    </section>
}